<?xml version="1.0"?>
<Queries xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Query OutputFileName="Result0.xml">
    <OrderBy>Year</OrderBy>
    <WhereClauses>
      <WhereClause PropertyName="Id" Type="LessThan">6</WhereClause>
      <WhereClause PropertyName="Year" Type="GreaterThan">1997</WhereClause>
    </WhereClauses>
  </Query>
  <Query OutputFileName="Result1.xml">
    <OrderBy>Year</OrderBy>
    <WhereClauses>
      <WhereClause PropertyName="Model" Type="Equals">6 Skyactiv</WhereClause>
      <WhereClause PropertyName="Year" Type="Equals">2014</WhereClause>
    </WhereClauses>
  </Query>
  </Queries>